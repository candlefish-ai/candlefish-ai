{
  "name": "@candlefish-ai/monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Candlefish AI - Illuminating the path to AI transformation",
  "workspaces": [
    "apps/*",
    "packages/*",
    "projects/*"
  ],
  "scripts": {
    "prebuild": "pnpm brand:sync",
    "brand:css": "node Branding/generate-css-vars.js",
    "dev": "turbo dev",
    "build": "turbo build",
    "build:website": "turbo build --filter=@candlefish-ai/website",
    "build:dashboard": "turbo build --filter=@candlefish-ai/dashboard",
    "dev:website": "turbo dev --filter=@candlefish-ai/website",
    "dev:dashboard": "turbo dev --filter=@candlefish-ai/dashboard",
    "brand:sync": "node Branding/sync-assets.js",
    "brand:sync:dry": "node Branding/sync-assets.js --dry-run",
    "brand:manifest": "node Branding/generate-archive-manifest.js",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:coverage": "./scripts/generate-coverage-report.sh",
    "test:coverage:upload": "./scripts/generate-coverage-report.sh --upload",
    "test:coverage:badges": "./scripts/generate-coverage-report.sh --badges",
    "test:backend": "jest --selectProjects \"Inventory Backend\"",
    "test:backend:inventory": "jest --selectProjects \"Inventory Backend\"",
    "test:frontend": "jest --selectProjects \"Inventory Frontend\"",
    "test:frontend:inventory": "jest --selectProjects \"Inventory Frontend\"",
    "test:graphql": "jest --selectProjects \"Inventory GraphQL\"",
    "test:mobile": "jest --selectProjects \"Inventory Mobile\"",
    "test:mobile:inventory": "jest --selectProjects \"Inventory Mobile\"",
    "test:integration": "jest --selectProjects \"Integration Tests\"",
    "test:accessibility": "jest --testPathPattern=__tests__/accessibility",
    "test:unit": "turbo test:unit",
    "test:e2e": "playwright test --config=playwright-inventory.config.ts",
    "test:e2e:ui": "playwright test --config=playwright-inventory.config.ts --ui",
    "test:e2e:cypress": "cypress run",
    "test:e2e:cypress:open": "cypress open",
    "test:performance": "k6 run __tests__/performance/k6-inventory-load-tests.js",
    "test:stress": "k6 run __tests__/performance/k6/stress-test.js",
    "test:lighthouse": "jest --testPathPattern=lighthouse-accessibility.test.js --runInBand",
    "test:pa11y": "jest --testPathPattern=pa11y-tests.test.js --runInBand",
    "test:all": "pnpm test && pnpm test:e2e && pnpm test:performance",
    "build:analyze": "turbo build:analyze",
    "clean": "turbo clean && rm -rf node_modules coverage test-results badges",
    "dev:frontend": "cd apps/nanda-dashboard && pnpm dev",
    "start:api": "cd apps/nanda-api && go run main.go",
    "docker:test": "docker-compose -f docker-compose.test.yml up --build --abort-on-container-exit",
    "docker:test:down": "docker-compose -f docker-compose.test.yml down -v",
    "deploy:website": "turbo deploy --filter=@candlefish-ai/website",
    "deploy:dashboard": "turbo deploy --filter=@candlefish-ai/dashboard",
    "deploy:all": "turbo deploy",
    "ci": "turbo build lint typecheck test"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-native": "^12.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.8",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-native": "^0.72.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "babel-jest": "^29.7.0",
    "codecov": "^3.8.3",
    "cypress": "^13.6.0",
    "eslint": "^8.53.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "faker": "^6.6.6",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nyc": "^15.1.0",
    "react-test-renderer": "^18.2.0",
    "supertest": "^6.3.3",
    "testcontainers": "^10.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "turbo": "^1.13.4",
    "typescript": "^5.2.2"
  },
  "pnpm": {
    "overrides": {
      "react-refresh": "~0.14.0",
      "d3-color@<3.1.0": ">=3.1.0",
      "axios@>=0.8.1 <0.28.0": ">=0.28.0",
      "ws@>=8.0.0 <8.17.1": ">=8.17.1",
      "send@<0.19.0": ">=0.19.0",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "esbuild@<=0.24.2": ">=0.25.0",
      "axios@<0.30.0": ">=0.30.0",
      "next@=14.2.25": ">=14.2.26",
      "cookie@<0.7.0": ">=0.7.0",
      "tar-fs@>=3.0.0 <3.0.7": ">=3.0.7",
      "tar-fs@>=2.0.0 <2.1.2": ">=2.1.2",
      "next@>=13.0 <14.2.30": ">=14.2.30",
      "prismjs@<1.30.0": ">=1.30.0",
      "tar-fs@>=3.0.0 <3.0.9": ">=3.0.9",
      "tar-fs@>=2.0.0 <2.1.3": ">=2.1.3"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.6",
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "chrome-launcher": "^0.15.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "lighthouse": "^11.3.0",
    "msw": "^2.0.8",
    "pa11y": "^7.0.0",
    "puppeteer": "^21.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
