# Zoom OAuth Configuration (Candlefish.ai Account)
CANDLEFISH_ZOOM_ACCOUNT_ID=your_candlefish_zoom_account_id
CANDLEFISH_ZOOM_CLIENT_ID=your_candlefish_zoom_client_id
CANDLEFISH_ZOOM_CLIENT_SECRET=your_candlefish_zoom_client_secret

# Read.ai Configuration
READ_AI_API_KEY=your_read_ai_api_key
READ_AI_API_URL=https://api.read.ai/v1

# AWS SES Configuration (Option 1)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Gmail Configuration (Option 2)
GMAIL_USER=your_email@gmail.com
GMAIL_APP_PASSWORD=your_app_password

# Email Provider Selection (ses or gmail)
EMAIL_PROVIDER=ses

# Application Settings
LOG_LEVEL=info
NODE_ENV=production