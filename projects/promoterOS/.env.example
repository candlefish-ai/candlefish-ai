# PromoterOS Environment Configuration Template
# Copy this to .env and fill in your actual values

# Application Environment
APP_ENV=production
APP_SECRET_KEY=your-secret-key-here
API_BASE_URL=https://promoteros.candlefish.ai
FRONTEND_URL=https://promoteros.candlefish.ai

# AI/LLM API Keys
ANTHROPIC_API_KEY=your_anthropic_key_here
OPENAI_API_KEY=your_openai_key_here
TOGETHER_API_KEY=your_together_key_here
FIREWORKS_API_KEY=your_fireworks_key_here

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost/promoteros
REDIS_URL=redis://localhost:6379

# TikTok API Configuration
TIKTOK_API_KEY=your_tiktok_api_key
TIKTOK_CLIENT_KEY=your_tiktok_client_key
TIKTOK_CLIENT_SECRET=your_tiktok_client_secret
TIKTOK_ORG_ID=your_tiktok_org_id
TIKTOK_ACCESS_TOKEN=your_tiktok_access_token

# Social Media APIs
INSTAGRAM_API_KEY=your_instagram_key
SPOTIFY_API_KEY=your_spotify_key
YOUTUBE_API_KEY=your_youtube_key
TWITTER_API_KEY=your_twitter_key

# Ticketing APIs
TICKETMASTER_API_KEY=your_ticketmaster_key
EVENTBRITE_API_KEY=your_eventbrite_key
AXS_API_KEY=your_axs_key

# AWS Configuration
AWS_REGION=us-west-2

# Monitoring
SENTRY_DSN=
DATADOG_API_KEY=

# Feature Flags
ENABLE_REAL_TIME_TRACKING=true
ENABLE_AUTO_BOOKING=false
ENABLE_PRICE_OPTIMIZATION=true

# Venue Configuration
DEFAULT_VENUE_CAPACITY_MIN=1200
DEFAULT_VENUE_CAPACITY_MAX=3500
DEFAULT_RADIUS_MILES=200

# Alert Thresholds
VIRAL_THRESHOLD_SOUND_USES=100000
VIRAL_THRESHOLD_WEEKLY_FOLLOWERS=30000
URGENT_BOOKING_WINDOW_DAYS=60
