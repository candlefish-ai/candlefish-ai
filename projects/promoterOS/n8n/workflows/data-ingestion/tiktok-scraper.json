        }
      }
    },
    {
      "parameters": {
        "channel": "#alerts",
        "text": "=ðŸš¨ *TikTok Scraping Error*\n\nâŒ Error: {{ $json.error.message }}\nðŸ“ Node: {{ $json.error.node }}\nðŸ• Time: {{ $now.toISO() }}\n\n*Stack Trace:*\n```\n{{ $json.error.stack }}\n```",
        "otherOptions": {
          "attachments": {
            "values": "=[{\n  \"color\": \"danger\",\n  \"fields\": [\n    {\n      \"title\": \"Workflow\",\n      \"value\": \"TikTok Data Ingestion\",\n      \"short\": true\n    },\n    {\n      \"title\": \"Environment\",\n      \"value\": \"{{ $env.ENVIRONMENT }}\",\n      \"short\": true\n    }\n  ]\n}]"
          }
        }
      },
      "id": "alert-error",
      "name": "Alert Error",
