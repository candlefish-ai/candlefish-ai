{
  "name": "paintbox-apollo-graphos",
  "version": "1.0.0",
  "description": "Paintbox paint estimation platform with Kind Home Paint integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run subgraph:estimates\" \"npm run subgraph:projects\" \"npm run subgraph:customers\" \"npm run subgraph:integrations\" \"npm run router\"",
    "build": "npm run build:estimates && npm run build:projects && npm run build:customers && npm run build:integrations",
    "start": "npm run build && npm run dev",
    "subgraph:estimates": "cd subgraph-estimates && npm run dev",
    "subgraph:projects": "cd subgraph-projects && npm run dev",
    "subgraph:customers": "cd subgraph-customers && npm run dev",
    "subgraph:integrations": "cd subgraph-integrations && npm run dev",
    "build:estimates": "cd subgraph-estimates && npm run build",
    "build:projects": "cd subgraph-projects && npm run build",
    "build:customers": "cd subgraph-customers && npm run build",
    "build:integrations": "cd subgraph-integrations && npm run build",
    "router": "rover dev --supergraph-config supergraph-local.yaml --router-config router/router.yaml",
    "compose": "rover supergraph compose --config supergraph-config.yaml > supergraph-schema.graphql",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:calculations": "ts-node scripts/validate-calculations.ts",
    "test:excel": "npm run test:calculations",
    "validate:schema": "rover subgraph check paintbox-graph@main --schema schema/enhanced-estimates.graphql --name estimates",
    "validate:calculations": "npm run test:calculations",
    "validate:all": "npm run validate:schema && npm run validate:calculations",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "k8s:logs": "kubectl logs -f deployment/estimates-deployment",
    "lint": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "generate:types": "graphql-codegen --config codegen.yml",
    "generate:docs": "graphql-markdown schema/enhanced-estimates.graphql > docs/schema.md"
  },
  "dependencies": {
    "@apollo/datasource-rest": "^6.2.2",
    "@apollo/gateway": "^2.5.6",
    "@apollo/server": "^4.9.5",
    "@apollo/subgraph": "^2.5.6",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.7",
    "apollo-server-express": "^3.12.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.14.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "pdf-lib": "^1.17.1",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "redis": "^4.6.10",
    "sharp": "^0.32.6",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/compression": "^1.7.5",
    "@types/config": "^3.3.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.19.10",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "concurrently": "^8.2.2",
    "docker-compose": "^0.24.2",
    "eslint": "^8.52.0",
    "graphql-markdown": "^7.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/candlefish-ai/paintbox-apollo-graphos.git"
  },
  "keywords": [
    "graphql",
    "apollo",
    "federation",
    "paint",
    "estimation",
    "kind-home-paint",
    "typescript"
  ],
  "author": "Candlefish AI",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/candlefish-ai/paintbox-apollo-graphos/issues"
  },
  "homepage": "https://github.com/candlefish-ai/paintbox-apollo-graphos#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "resolvers/**/*.ts",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  }
}
