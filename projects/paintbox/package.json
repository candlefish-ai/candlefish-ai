{
  "name": "paintbox-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development ts-node -r tsconfig-paths/register --project tsconfig.server.json server/websocket-server.ts",
    "dev:next": "NEXT_DISABLE_TURBOPACK=1 next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=16384 --max-semi-space-size=512' next build",
    "build:production": "NODE_OPTIONS='--max-old-space-size=32768 --max-semi-space-size=1024' SKIP_STATIC_OPTIMIZATION=1 DISABLE_ESLINT=1 DISABLE_TYPESCRIPT_CHECK=1 next build",
    "build:deploy": "NODE_OPTIONS='--max-old-space-size=2048' SKIP_STATIC_OPTIMIZATION=1 DISABLE_ESLINT=1 DISABLE_TYPESCRIPT_CHECK=1 next build",
    "build:chunked": "NODE_OPTIONS='--max-old-space-size=32768 --max-semi-space-size=1024' NEXT_BUILD_SPLIT_CHUNKS=1 DISABLE_STATIC_OPTIMIZATION=1 next build",
    "build:dynamic": "NODE_OPTIONS='--max-old-space-size=32768 --max-semi-space-size=1024' DISABLE_STATIC_OPTIMIZATION=1 DISABLE_ESLINT=1 DISABLE_TYPESCRIPT_CHECK=1 next build",
    "export": "next export",
    "build:full": "next build && npx tsc server/websocket-server.ts --outDir dist --esModuleInterop --module commonjs",
    "build:websocket": "npx tsc server/websocket-server.ts --outDir dist --esModuleInterop --module commonjs",
    "start": "next start -p 8080 -H 0.0.0.0",
    "start:ws": "NODE_ENV=production node dist/websocket-server.js",
    "start:next": "next start",
    "postinstall": "prisma generate || true",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest --testPathPatterns=security",
    "test:performance": "jest --testPathPatterns=performance",
    "test:integration": "jest --testPathPatterns=integration",
    "test:components": "jest --testPathPatterns=components",
    "test:api": "jest --testPathPatterns=api",
    "test:accessibility": "jest --testPathPatterns=accessibility",
    "test:production": "./__tests__/scripts/run-production-tests.sh",
    "test:production:unit": "jest --testPathPatterns='api/production'",
    "test:production:integration": "jest --testPathPatterns='integration'",
    "test:production:components": "jest --testPathPatterns='components/production'",
    "test:production:security": "jest --testPathPatterns='security'",
    "test:production:accessibility": "jest --testPathPatterns='accessibility'",
    "test:production:performance": "jest --testPathPatterns='performance'",
    "test:production:coverage": "jest --coverage --testPathPatterns='(api/production|integration|components/production|security|accessibility)'",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:load": "artillery run artillery-config.yml",
    "test:load:report": "artillery run artillery-config.yml --output report.json && artillery report report.json",
    "test:companycam": "ts-node scripts/test-companycam.ts",
    "test:salesforce": "ts-node scripts/test-salesforce.ts",
    "test:salesforce:integration": "ts-node -r tsconfig-paths/register scripts/test-salesforce-integration.ts",
    "test:calculations": "ts-node scripts/test-calculations.ts",
    "test:excel-basic": "tsx scripts/test-excel-engine-basic.ts",
    "test:excel-integration": "tsx scripts/test-excel-engine-integration.ts",
    "test:excel-parity": "tsx scripts/excel-parity-test.ts",
    "test:excel-validation": "tsx scripts/test-excel-validation-simple.ts",
    "create:test-files": "tsx scripts/create-sample-test-files.ts",
    "test:all": "npm run test && npm run test:e2e && npm run test:load",
    "test:frontend:coverage": "jest --testPathPatterns=components --coverage --coverageDirectory=coverage/frontend",
    "test:backend:coverage": "jest --testPathPatterns='(resolvers|pricing|subscriptions)' --coverage --coverageDirectory=coverage/backend",
    "test:mobile:coverage": "cd mobile/PaintboxMobile && npm run test:coverage",
    "test:integration:coverage": "jest --testPathPatterns=integration --coverage --coverageDirectory=coverage/integration --testTimeout=30000",
    "test:e2e:setup": "playwright install && npm run db:test:setup",
    "test:e2e:start-servers": "npm run build && npm run start:test",
    "test:e2e:run": "playwright test",
    "test:perf:setup": "npm run db:test:setup",
    "test:perf:seed": "node __tests__/data/test-data-setup.js seed",
    "test:perf:start-servers": "npm run start:test",
    "test:perf:check-regression": "node scripts/check-performance-regression.js",
    "test:a11y": "jest --testPathPatterns=accessibility",
    "test:smoke": "jest --testPathPatterns=smoke --testTimeout=60000",
    "test:data:seed": "node __tests__/data/test-data-setup.js seed",
    "test:data:validate": "node __tests__/data/test-data-setup.js validate",
    "test:data:cleanup": "node __tests__/data/test-data-setup.js cleanup",
    "test:data:summary": "node __tests__/data/test-data-setup.js summary",
    "db:test:setup": "NODE_ENV=test npx prisma migrate reset --force && npx prisma db push",
    "start:test": "NODE_ENV=test npm run start",
    "type-check": "tsc --noEmit",
    "deploy:test": "echo 'Deploy to test environment'",
    "sync:start": "ts-node scripts/start-sync.ts",
    "sync:stop": "ts-node scripts/stop-sync.ts",
    "deploy:render": "./scripts/render-deploy.sh full",
    "deploy:validate": "./scripts/render-deploy.sh validate",
    "deploy:secrets": "./scripts/render-deploy.sh secrets",
    "deploy:health": "./scripts/render-deploy.sh health",
    "deploy:monitor": "./scripts/render-deploy.sh monitor --watch",
    "deploy:rollback": "./scripts/render-deploy.sh rollback",
    "deploy:status": "./scripts/render-deploy.sh status",
    "deploy:logs": "./scripts/render-deploy.sh logs"
  },
  "dependencies": {
    "@candlefish/jwt-auth": "file:../../packages/jwt-auth",
    "@anthropic-ai/sdk": "^0.57.0",
    "@apollo/client": "^3.13.9",
    "@auth/prisma-adapter": "^2.7.4",
    "@aws-sdk/client-secrets-manager": "^3.857.0",
    "@bull-board/express": "^6.12.0",
    "@heroicons/react": "^2.1.5",
    "@infisical/sdk": "^4.0.4",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-pdf/renderer": "^4.3.0",
    "@react-spring/web": "^10.0.1",
    "@sendgrid/mail": "^8.1.5",
    "@sentry/nextjs": "^10.0.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/bull": "^3.15.9",
    "@types/exceljs": "^0.5.3",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.9",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@use-gesture/react": "^10.3.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "bull": "^4.16.5",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "decimal.js": "^10.6.0",
    "dexie": "^4.0.11",
    "exceljs": "^4.4.0",
    "formula-parser": "^2.0.1",
    "framer-motion": "^11.17.0",
    "fuse.js": "^7.1.0",
    "graphql": "^16.10.0",
    "graphql-ws": "^6.0.6",
    "idb": "^8.0.1",
    "ioredis": "^5.7.0",
    "isomorphic-dompurify": "2.15.0",
    "jose": "^5.10.0",
    "jsdom": "^26.0.0",
    "jsforce": "^3.9.5",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "logrocket": "^10.1.0",
    "logrocket-react": "^6.0.3",
    "lucide-react": "^0.534.0",
    "mathjs": "^14.6.0",
    "next": "15.4.5",
    "next-auth": "^4.24.10",
    "nodemailer": "^7.0.5",
    "p-limit": "^6.2.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.1.1",
    "postcss": "^8.5.3",
    "prisma": "^6.13.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "react-pdf": "^10.0.1",
    "redis": "^5.7.0",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
