{
  "name": "system-analyzer-graphql",
  "version": "1.0.0",
  "description": "GraphQL API for System Analyzer - 'run all open so we can analyze status'",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "generate:types": "graphql-codegen",
    "introspect": "apollo schema:download --endpoint=http://localhost:4000/graphql schema.json",
    "validate": "apollo schema:check --endpoint=http://localhost:4000/graphql",
    "studio": "apollo service:push --endpoint=http://localhost:4000/graphql"
  },
  "keywords": [
    "graphql",
    "system-monitoring",
    "microservices",
    "real-time",
    "status-analysis",
    "federation",
    "apollo-server"
  ],
  "author": "System Analyzer Team",
  "license": "MIT",
  "dependencies": {
    "@apollo/gateway": "^2.5.0",
    "@apollo/federation": "^0.38.1",
    "apollo-server-express": "^3.12.0",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.2",
    "graphql-subscriptions": "^2.0.0",
    "graphql-redis-subscriptions": "^2.6.0",
    "dataloader": "^2.2.2",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.8.1",
    "express-slow-down": "^1.6.0",
    "ioredis": "^5.3.2",
    "subscriptions-transport-ws": "^0.11.0",
    "dockerode": "^3.3.5",
    "axios": "^1.4.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.0",
    "@types/express": "^4.17.17",
    "@types/compression": "^1.7.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "@types/jest": "^29.5.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.0",
    "apollo": "^2.34.0",
    "graphql-query-complexity": "^0.12.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/candlefish-ai/paintbox.git",
    "directory": "lib/graphql"
  },
  "bugs": {
    "url": "https://github.com/candlefish-ai/paintbox/issues"
  },
  "homepage": "https://github.com/candlefish-ai/paintbox#readme"
}
