{
  "framework": "nextjs",
  "buildCommand": "npm run build:dynamic",
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=32768 --max-semi-space-size=1024",
      "NODE_ENV": "production",
      "NEXT_TELEMETRY_DISABLED": "1",
      "DISABLE_STATIC_OPTIMIZATION": "1"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60,
      "memory": 3008
    },
    "app/**/page.tsx": {
      "maxDuration": 30,
      "memory": 1769
    }
  },
  "outputDirectory": ".next",
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|woff|woff2|ttf|eot))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Vary",
          "value": "Accept-Encoding"
        }
      ]
    },
    {
      "source": "/(.*\\.(jpg|jpeg|png|gif|ico|svg|webp|avif))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000, immutable"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "version": 2,
  "regions": [
    "iad1",
    "sfo1"
  ]
}