{
  "dashboard": {
    "id": null,
    "title": "Candlefish Website - Production Monitoring",
    "tags": ["candlefish", "production", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "collapsed": false
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"candlefish-website\"}[5m]))",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 500}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 1}
      },
      {
        "id": 3,
        "title": "Response Time (95th percentile)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"candlefish-website\"}[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 2}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 1}
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"candlefish-website\",status=~\"5..\"}[5m])) / sum(rate(http_requests_total{job=\"candlefish-website\"}[5m])) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 1}
      },
      {
        "id": 5,
        "title": "Active Pods",
        "type": "stat",
        "targets": [
          {
            "expr": "count(up{job=\"candlefish-website\"} == 1)",
            "legendFormat": "Healthy Pods",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 2},
                {"color": "green", "value": 3}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 1}
      },
      {
        "id": 6,
        "title": "Application Performance",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9},
        "collapsed": false
      },
      {
        "id": 7,
        "title": "Request Rate Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"candlefish-website\"}[5m])) by (status)",
            "legendFormat": "{{status}} codes",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 10}
      },
      {
        "id": 8,
        "title": "Response Time Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{job=\"candlefish-website\"}[5m])) by (le))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"candlefish-website\"}[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=\"candlefish-website\"}[5m])) by (le))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (s)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10}
      },
      {
        "id": 9,
        "title": "Infrastructure Metrics",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 18},
        "collapsed": false
      },
      {
        "id": 10,
        "title": "CPU Usage by Pod",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(container_cpu_usage_seconds_total{pod=~\"candlefish-website.*\"}[5m])) by (pod) * 100",
            "legendFormat": "{{pod}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "CPU %",
            "min": 0,
            "max": 100
          }
        ],
        "alert": {
          "conditions": [
            {
              "query": {"queryType": "", "refId": "A"},
              "reducer": {"type": "avg", "params": []},
              "evaluator": {"params": [70], "type": "gt"}
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "High CPU Usage",
          "noDataState": "no_data",
          "notifications": []
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 19}
      },
      {
        "id": 11,
        "title": "Memory Usage by Pod",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(container_memory_working_set_bytes{pod=~\"candlefish-website.*\"}) by (pod) / 1024 / 1024",
            "legendFormat": "{{pod}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Memory (MB)",
            "min": 0
          }
        ],
        "alert": {
          "conditions": [
            {
              "query": {"queryType": "", "refId": "A"},
              "reducer": {"type": "avg", "params": []},
              "evaluator": {"params": [400], "type": "gt"}
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Memory Usage",
          "noDataState": "no_data",
          "notifications": []
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 19}
      },
      {
        "id": 12,
        "title": "Business Metrics",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 27},
        "collapsed": false
      },
      {
        "id": 13,
        "title": "Assessment Completions",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(candlefish_assessments_total[1h])",
            "legendFormat": "Assessments/hour",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 28}
      },
      {
        "id": 14,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "candlefish_users_active",
            "legendFormat": "Active Users",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 28}
      },
      {
        "id": 15,
        "title": "Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "cache_hits_total / (cache_hits_total + cache_misses_total) * 100",
            "legendFormat": "Hit Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 90}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 28}
      },
      {
        "id": 16,
        "title": "Database Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "database_connections_active",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 28}
      },
      {
        "id": 17,
        "title": "External Dependencies",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 36},
        "collapsed": false
      },
      {
        "id": 18,
        "title": "Database Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(postgres_query_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (s)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 37}
      },
      {
        "id": 19,
        "title": "Redis Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(redis_command_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (s)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 37}
      },
      {
        "id": 20,
        "title": "CDN Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "cloudfront_cache_hit_rate",
            "legendFormat": "CloudFront Hit Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 37}
      },
      {
        "id": 21,
        "title": "Alerts & Incidents",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 45},
        "collapsed": false
      },
      {
        "id": 22,
        "title": "Active Alerts",
        "type": "table",
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\"}",
            "format": "table",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 46}
      },
      {
        "id": 23,
        "title": "Error Logs (Last Hour)",
        "type": "logs",
        "targets": [
          {
            "expr": "{namespace=\"production\",app=\"candlefish-website\"} |= \"ERROR\"",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 46}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "constant",
          "query": "production",
          "current": {
            "value": "production",
            "text": "production"
          }
        },
        {
          "name": "namespace",
          "type": "query",
          "query": "label_values(kube_pod_info, namespace)",
          "current": {
            "value": "production",
            "text": "production"
          },
          "refresh": 1
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "changes(candlefish_deployment_info[1h])",
          "iconColor": "green",
          "tags": ["deployment"]
        },
        {
          "name": "Alerts",
          "datasource": "Prometheus", 
          "expr": "ALERTS{alertstate=\"firing\"}",
          "iconColor": "red",
          "tags": ["alert"]
        }
      ]
    }
  }
}