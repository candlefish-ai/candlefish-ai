{
  "name": "candlefish-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "refresh-mocks": "node scripts/refreshMocks.js",
    "build-workshop-index": "ts-node scripts/buildWorkshopIndex.ts",
    "validate-workshop": "ts-node scripts/validateWorkshop.ts",
    "prebuild": "npm run build-workshop-index && npm run validate-workshop",
    "dev": "npm run refresh-mocks && npm run build-workshop-index && next dev",
    "build": "npm run refresh-mocks && npm run prebuild && next build",
    "export": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "export-pdf": "node scripts/export-pdf.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:accessibility": "playwright test --grep @accessibility",
    "test:performance": "k6 run tests/performance/load-test.js",
    "test:visual": "playwright test --grep @visual",
    "test:all": "npm run test:ci && npm run test:e2e && npm run test:accessibility"
  },
  "dependencies": {
    "@headlessui/react": "1.7.18",
    "@heroicons/react": "2.1.1",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "10.4.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^10.18.0",
    "gray-matter": "4.0.3",
    "html2canvas": "^1.4.1",
    "next-mdx-remote": "^4.4.1",
    "jspdf": "^3.0.1",
    "next": "^14.2.32",
    "p5": "^1.11.9",
    "postcss": "8.4.33",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^13.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark": "15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "16.0.1",
    "resend": "^4.0.0",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "3.4.1",
    "three": "^0.179.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@cypress/code-coverage": "^3.14.6",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "20.11.5",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "artillery": "^2.0.24",
    "axios-mock-adapter": "^2.1.0",
    "critters": "^0.0.23",
    "cypress": "^14.5.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.32",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "k6": "^0.0.0",
    "msw": "^2.10.5",
    "node-fetch": "^2.7.0",
    "playwright": "^1.54.2",
    "raw-loader": "^4.0.2",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  }
}
