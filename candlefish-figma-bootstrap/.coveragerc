[run]
source = plugin/src, scripts
omit =
    */node_modules/*
    */dist/*
    */build/*
    */__tests__/*
    */coverage/*
    *.test.ts
    *.spec.ts

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
