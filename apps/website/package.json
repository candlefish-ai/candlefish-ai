{
  "name": "@candlefish-ai/website",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Candlefish AI main website",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "deploy": "netlify deploy --prod --dir=dist",
    "family:dev": "vite --config vite.family.config.ts",
    "family:build": "vite build --config vite.family.config.ts",
    "preview:serve": "vite preview --outDir dist --port 4173 --strictPort",
    "test:figma": "node scripts/test-figma-api.mjs",
    "test:performance": "node scripts/performance-test.js",
    "analyze": "ANALYZE=true vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:api": "vitest run --config vitest.api.config.ts",
    "test:accessibility": "vitest run --testNamePattern=\"accessibility\"",
    "test:all": "npm run test:run && npm run test:e2e && npm run test:accessibility"
  },
  "dependencies": {
    "@candlefish-ai/ui-components": "^1.0.0",
    "@react-spring/web": "^10.0.1",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@types/three": "^0.178.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^10.18.0",
    "gsap": "^3.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^3.1.4",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^6.24.1",
    "react-spring": "^10.0.1",
    "three": "^0.178.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@netlify/plugin-lighthouse": "^6.0.1",
    "@playwright/test": "^1.47.0",
    "@radix-ui/react-dialog": "^1.0.6",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-tabs": "^1.0.5",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "autoprefixer": "^10.4.19",
    "axe-core": "^4.10.0",
    "axe-playwright": "^2.0.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "figma-api": "^1.11.0",
    "jsdom": "^24.1.1",
    "lighthouse": "^12.2.0",
    "lucide-react": "^0.424.0",
    "msw": "^2.3.5",
    "postcss": "^8.4.39",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwind-merge": "^2.5.2",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.3",
    "vite": "^5.4.19",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^2.0.5"
  }
}
